{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "dummyreaction-config",
  "type": "object",
  "description": "This reaction does _nothing_ and is only here to demonstrate the syntax.",
  "additionalProperties": false,
  "properties": {
    "example": {
      "type": "number",
      "description": "This is an example of an option for a reaction.",
      "pytype": "float",
      "default": 0.0
    },
    "example2": {
      "type": "string",
      "pytype": "str",
      "description": "This is another example of an option for a reaction.",
      "default": "hello"
    },
    "kmc": {
      "description": "KMC algorithm for this reaction.",
      "type": "string",
      "pytype": "str",
      "enum": ["rfkmc", "frm", "extrande"],
      "default": "extrande"
    }
  },
  "required": [  ]
}

